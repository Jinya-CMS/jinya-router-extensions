{
  "name": "jinya/router-extensions",
  "description": "Official extensions for the Jinya Router",
  "type": "library",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "jinya/database": "^0.17.0",
    "jinya/router": "^0.10.0"
  },
  "require-dev": {
    "laravel/pint": "v1.15.0",
    "symfony/filesystem": "v7.0.3"
  },
  "suggest": {
    "ext-zend opcache": "*"
  },
  "license": "MIT",
  "autoload": {
    "files": [
      "src/Database/routing_handler_utils.php"
    ],
    "psr-4": {
      "Jinya\\Router\\Extensions\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/Database/database_utils.php"
    ],
    "psr-4": {
      "Jinya\\Router\\Extensions\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Imanuel Ulbricht",
      "email": "imanuel.ulbricht@jinya.de"
    }
  ]
}
